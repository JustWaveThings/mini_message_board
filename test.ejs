<div>
  <% if(!messages) { %>
  <h2>No messages yet!</h2>
  <% } else { messages.forEach(function(message) { %>
  <div class="message-cont">
    <h2 class="user"><%= message.user %></h2>
    <p class="message"><%= message.text %></p>
    <p class="added"><%= message.added %></p>
  </div>
  <% })} %>
</div>

